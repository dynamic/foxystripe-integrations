{
  "name": "dynamic/foxystripe-integrations",
  "description": "FoxyStripe Integrations is an add-on to FoxyStripe that allows integrations with the Datafeed.",
  "keywords": ["silverstripe", "foxycart", "foxystripe", "datafeed", "dynamic", "ecommerce"],
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Dynamic Inc",
      "email": "dev@dynamicagency.com",
      "homepage": "http://www.dynamicagency.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "dynamic/foxystripe": "^4.0",
    "silverstripe/recipe-core": "^4.2",
    "silverstripe/vendor-plugin": "^1.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^5.7",
    "squizlabs/php_codesniffer": "*"
  },
  "config": {
    "process-timeout": 600
  },
  "autoload": {
    "psr-4": {
      "Dynamic\\FoxyStripe\\": "src/",
      "Dynamic\\FoxyStripe\\Test\\": "tests/"
    }
  },
  "extra": {
    "installer-name": "foxystripe-integrations",
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
